    <script>
    $(document).ready(function () {
        $('#datePicker')
            .datepicker({
                format: 'dd/mm/yyyy'
            })
            .on('changeDate', function (e) {
                // Revalidate the date field
                $('#eventForm').formValidation('revalidateField', 'date');
            });
        var i = 1;
        $("#add_row").click(function () {
            $('#addr' + i).html("<td>" + (i + 1) + "</td><td><input type='text' name='fecha" + i + "' placeholder='Ingrese la fecha' class='form-control' /></td><td><input name='detalle" + i + "' type='text' placeholder='Ingrese el detalle del gasto' class='form-control input-md'  /> </td><td><input type='text' name='boleta" + i  +"' placeholder='Ingrese el numero de boleta' class='form-control' /></td><td><input  name='costo" + i + "' type='text' placeholder='Ingrese el monto del gasto'  class='form-control input-md'></td>");

            $('#tab_logic').append('<tr id="addr' + (i + 1) + '"></tr>');
            i++;
        });
        $("#delete_row").click(function () {
            if (i > 1) {
                $("#addr" + (i - 1)).html('');
                i--;
            }
        });
    });
    </script>

