@model IEnumerable<AccesoDatos.Models.Auspiciador>

<section id="auspiciadores" class="secPad">
    <div class="container">
        <div class="heading text-center">
            <!-- Heading -->
            <h2>Auspiciadores</h2>
            <p>Conoce quienes son parte de mis sueños y logros</p>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-xs-6 col-sm-3" style="margin-bottom: 10px;">
                    <div class=" thumbnail">
                        <a href="@item.website" target="_blank"><img src="@item.url_logo" style="min-height:210px;height:210px;min-width:245px;width:245px;" alt="Flayer"></a>
                        <div class="caption">
                            <a href="@item.website" target="_blank"><h3>@item.nombre</h3></a>
                            <p>@item.descripcion</p>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</section>